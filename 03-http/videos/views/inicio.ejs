<style>
    table, th,td{
        border:1px solid cornflowerblue;

    }
</style>

<%var apellido = 'Cardenas'%>
<h1> <%=nombre%> <%=apellido%> </h1>

<p>Parrafo</p>
<!-- ul unordered list -->
<!-- ol ordered list -->

<ol>
    <li>1</li>
    <li>2</li>
    <li>3</li>
</ol>
<h2>Arreglo</h2>

<ul>
<%arreglo.forEach((numero)=>{ %>
<li><%=numero%></li>
<%})%>
</ul>

<table>
    <!-- tr table row -->
    <tr>
        <th>Identificador</th>
        <th>Nombre</th>
        <th>Acciones</th>
    </tr>
    <% arreglo.forEach((usuario)=>{%>
    <tr>
        <td><%=usuario.id%></td>
        <td><%=usuario.nombre%></td>
        <td><form action="/Usuario/borrar/<%=usuario.id%>"
            method ="post">
                <button type="button">Yo no</button>
                <button>Yo si</button>
                <button type= "submit">Eliminar</button>
            </form></td>
    </tr>
    <%  }) %>

</table>